    {
      "name": "infra.telemetry.tracing.processor-properties",
      "sourceType": "com.labijie.infra.telemetry.configuration.tracing.TracingProperties",
      "type": "java.util.Properties"
    },
    {
      "name": "infra.telemetry.tracing.built-in-exporter",
      "sourceType": "com.labijie.infra.telemetry.configuration.tracing.TracingProperties",
      "type": "com.labijie.infra.telemetry.configuration.tracing.BuiltInExport",
      "defaultValue": "Kafka"
    },

    {
      "name": "infra.telemetry.metric.enabled",
      "sourceType": "com.labijie.infra.telemetry.configuration.metric.MetricProperties",
      "type": "java.lang.Boolean",
      "defaultValue": true
